{
  "name": "full-stack-collaboration-hub",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Starter scaffold for the Full-Stack Collaboration Hub assignment.",
  "engines": {
    "node": "18.19.0",
    "npm": "10.2.4"
  },
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "node src/server/main.js",
    "start:client": "vite",
    "dev": "vite",
    "build": "vite build",
    "seed": "node database/seed.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,vue,json,md}\"",
    "validate": "node validation_runner.js --db-path=data/app.db"
  },
  "dependencies": {
    "axios": "1.6.8",
    "concurrently": "8.2.2",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "7.1.5",
    "jsonwebtoken": "9.0.2",
    "sqlite3": "5.1.6",
    "vue": "3.4.15",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.19.2",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "vite": "5.0.10",
    "vitest": "0.34.6"
  }
}

